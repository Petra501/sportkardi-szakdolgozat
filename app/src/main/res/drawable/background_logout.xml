<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="?colorPrimary"/>
            <size
                android:width="4dp"
                android:height="4dp"/>
            <stroke
                android:width="2dp"
                android:color="?backgroundColor"/>
        </shape>
    </item>

    <item
        android:drawable="@drawable/ic_logout"
        android:top="12dp"
        android:bottom="12dp"
        android:right="12dp"
        android:left="12dp"
    />
</layer-list>